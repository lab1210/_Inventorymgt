<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
    <meta name="description" content="POS - Bootstrap Admin Template">
    <meta name="keywords"
          content="admin, estimates, bootstrap, business, corporate, creative, management, minimal, modern,  html5, responsive">
    <meta name="author" content="Dreamguys - Bootstrap Admin Template">
    <meta name="robots" content="noindex, nofollow">
    <title>SmartShelf</title>


    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/fontawesome")


</head>
<body>
    <div id="global-loader">
        <div class="whirly-loader"> </div>
    </div>

    <div class="main-wrapper">
        <div class="header">
            @Html.Partial("_header")
        </div>
        @Html.Partial("_SideNav")


        <div class="page-wrapper">
            <div class="content">
                @RenderBody()
            </div>
        </div>

    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/Feather")

    @Scripts.Render("~/bundles/slimscroll")
    @Scripts.Render("~/bundles/jqueryDataTables")
    @Scripts.Render("~/bundles/dataTablesBootstrap")
    @Scripts.Render("~/bundles/bootstrap5")
    @Scripts.Render("~/bundles/apexchart")
    @Scripts.Render("~/bundles/owl")
    @Scripts.Render("~/bundles/script")
    @Scripts.Render("~/bundles/select")
    @Scripts.Render("~/bundles/moment")
    @Scripts.Render("~/bundles/datetime")
    @Scripts.Render("~/bundles/sweetalert")




    @RenderSection("scripts", required: false)
</body>
</html>
